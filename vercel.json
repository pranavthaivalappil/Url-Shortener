{
  "version": 2,
  "buildCommand": "cd client-app && npm install && npm run build",
  "outputDirectory": "client-app/dist",
  "functions": {
    "server-app/api/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-app/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}